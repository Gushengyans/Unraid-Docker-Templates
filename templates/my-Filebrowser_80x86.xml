<?xml version="1.0"?>
<Container version="2">
  <Name>Filebrowser_80x86</Name>
  <Repository>80x86/filebrowser:2.9.4-amd64</Repository>
  <Registry>https://hub.docker.com/r/80x86/filebrowser/</Registry>
  <Network>host</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/80x86/filebrowser/</Support>
  <Project/>
  <Overview> filebrowser enhanced v2.9.0 for amd64/arm64 machine   Converted By Community Applications   Always verify this template (and values) against the dockerhub support page for the container</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:8082]</WebUI>
  <TemplateURL/>
  <Icon>https://ghproxy.com/raw.githubusercontent.com/xushier/HD-Icons/main/border-radius/FileBrowser.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1656992237</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="UMASK_SET" Target="UMASK_SET" Default="000" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">000</Config>
  <Config Name="Files" Target="/myfiles" Default="/mnt/user" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user</Config>
  <Config Name="Config" Target="/config" Default="/mnt/user/appdata/Filebrowser/config" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/Filebrowser/config</Config>
  <Config Name="PUID" Target="PUID" Default="1000" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">0</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">0</Config>
  <Config Name="WEBUI" Target="1611" Default="1611" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">1611</Config>
  <Config Name="UHD630" Target="" Default="/dev/dri/renderD128" Mode="" Description="" Type="Device" Display="always" Required="false" Mask="false">/dev/dri/renderD128</Config>
  <!-- <Config Name="Link" Target="/mnt" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user</Config> PS: Vertex symbol link need -->
</Container>
