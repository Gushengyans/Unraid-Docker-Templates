<?xml version="1.0"?>
<Container version="2">
  <Name>Redis</Name>
  <Repository>redis:alpine</Repository>
  <Registry>https://registry.hub.docker.com/_/redis/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://lime-technology.com/forums/topic/69490-support-jj9987-redis/</Support>
  <Project>https://redis.io/</Project>
  <Overview>
    Redis is an open source (BSD licensed), in-memory data structure store, used as a database, cache and message broker. Based on official Redis Docker image.
  </Overview>
  <Category>Network:Management Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:1211]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/juusujanar/unraid-templates/master/Redis.xml</TemplateURL>
  <Icon>https://ghproxy.com/raw.githubusercontent.com/xushier/HD-Icons/main/border-radius/Redis.png</Icon>
  <ExtraParams>--hostname redis</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1647946011</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>&#xD;
    Redis is an open source (BSD licensed), in-memory data structure store, used as a database, cache and message broker. Based on official Redis Docker image.&#xD;
  </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1211</HostPort>
        <ContainerPort>6379</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/Redis</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Redis Port" Target="6379" Default="1211" Mode="tcp" Description="This is Redis default connection port." Type="Port" Display="always-hide" Required="true" Mask="null">1211</Config>
  <Config Name="Data" Target="/data" Default="/mnt/user/appdata/Redis" Mode="rw" Description="/mnt/user/appdata/Redis" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/Redis</Config>
</Container>
